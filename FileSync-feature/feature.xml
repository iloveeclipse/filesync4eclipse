<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="FileSync"
      label="FileSync"
      version="1.3.10.qualifier"
      provider-name="Andrey Loskutov"
      plugin="de.loskutov.FileSync">

   <description url="https://github.com/iloveeclipse/filesync4eclipse/wiki">
      FileSync plugin for Eclipse is a file synchronisation tool. The
main goal is to keep files outside of Eclipse projects in-sync
with Eclipse project files.
   </description>

